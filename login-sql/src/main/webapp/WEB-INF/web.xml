<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <!--
  <display-name>NAME OF APP</display-name>
  -->
  
  <!-- SERVLET INFO
  <servlet>
    <servlet-name>SERVLETNAME</servlet-name>
    <servlet-class>PACKAGE.CLASSFILE</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  -->
  
  <!-- MAP SERVLET TO URI PATTERN (www.www.www/APPNAME/URI-PATTERN)
  <servlet-mapping>
    <servlet-name>SERVLETNAME</servlet-name>
    <url-pattern>/URI-PATTERN</url-pattern>
  </servlet-mapping>
  -->
  
  <!-- INDEX FILE FOR WEBAPP
  <welcome-file-list>
    <welcome-file>INDEX.JSP.XHTML</welcome-file>
  </welcome-file-list>
  -->
</web-app>
